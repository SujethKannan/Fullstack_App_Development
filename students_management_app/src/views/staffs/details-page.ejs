<%- include('../header')  %>   

<h3>STAFFS SECTION</h3>
 
<div class="container">

  
  <table>
      <tr>
        <th>ID</th>
        <th>NAME</th>
        <th>SUBJECT</th>
        <th>DEPARTMENT</th>
        <th>MOBILE_NO</th>
        <th>EMAIL</th>
      </tr>
      <% for (const d of data) { %>
      <tr>
        <td><%=d.ID%></td>
        <td><%=d.NAME%></td>
        <td><%=d.SUBJECT%></td>
        <td><%=d.DEPARTMENT%></td>
        <td><%=d.MOBILE_NO%></td>
        <td><%=d.EMAIL%></td>
      </tr>
      <% } %>
    </table>
    <a class ="btn btn-secondary" href="/app/staffs">Back</Details></a>

</div>
  


<%- include('../footer')  %>
 