<%- include('../header')  %>   
   
<h3>STUDENTS SECTION</h3>

<div class="container">

  <style>
    table {
       border:1px solid black;
       padding: 1px;
    }
    th, td{
       border:1px solid black;
       padding: 2px;
    }
 </style>
  <center> <h4>STUDENTS DETAILS PAGE</h4></center>
  <table style="width: 100%">
    <tr >
      <th>ID</th>
      <th>NAME</th>
      <th>STREAM</th>
      <th>DEPARTMENT</th>
      <th>BATCH</th>
      <th>MOBILE_NO</th>
      <th>EMAIL</th>
      <th>BLOOD_GROUP</th>
    </tr>
    <% for (const d of data) { %>
    <tr>
      <td><%=d.STUDENT_ID%></td>
      <td><%=d.NAME%></td>
      <td><%=d.STREAM%></td>
      <td><%=d.DEPARTMENT%></td>
      <td><%=d.BATCH%></td>
      <td><%=d.MOBILE_NO%></td>
      <td><%=d.EMAIL%></td>
      <td><%=d.BLOOD_GROUP%></td>
    </tr>
    <% } %>
  </table>
  <br>
  <a class ="btn btn-secondary" href="/app/students">Back</Details></a>

</div>

  


<%- include('../footer')  %>
 